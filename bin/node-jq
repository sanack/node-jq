#!/usr/bin/env node

const { spawn } = require('child_process')
const path = require('path')

const jq = process.platform === 'win32' ? 'jq.exe' : 'jq'

const child = spawn(
  path.join(__dirname, jq),
  process.argv.slice(2),
  { stdio: 'inherit' }
)

child.on('exit', (code) => process.exit(code))
